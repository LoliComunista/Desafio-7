package inteiromaior;

import java.util.Scanner;

public class InteiroMaior {

    public static void main(String[] args) {
            //variaveis
        Scanner ler=new Scanner(System.in);
       numMenor();
    }
    public static void numMenor(){
        //variaveis
        Scanner ler=new Scanner(System.in);
        int num1, num2;
        
        //dados
        System.out.println("Insira dois n√∫mero");
        num1=ler.nextInt();
        num2=ler.nextInt();
        //processo
        if(num1<num2){
            System.out.println(num1+" Maior");
        }else{
            System.out.println(num2+"   Maior");
        
        }
    }
    
}
